{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "regions": [
        "fra1"
    ],
    "functions": {
        "trackme_backend/wsgi.py": {
            "runtime": "@vercel/python",
            "memory": 128
        }
    },
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "trackme_backend/wsgi.py"
        }
    ]
}