{
    "builds": [
        {
            "src": "trackme_backend/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9",
                "regions": "fra1"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "trackme_backend/wsgi.py"
        }
    ]
}